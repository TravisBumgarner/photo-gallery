{
  "name": "photo-gallery",
  "version": "1.0.0",
  "description": "Full-stack app with React, Vite, Express, and Drizzle ORM",
  "workspaces": [
    "shared",
    "backend",
    "ingestion",
    "frontend"
  ],
  "scripts": {
    "setup": "bash bootstrap.sh",
    "dev:backend": "npm run dev -w backend",
    "dev:frontend": "npm run dev -w frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "npm run build -w backend && npm run build -w frontend",
    "db:generate": "npm run db:generate -w backend",
    "db:migrate": "npm run db:migrate -w backend",
    "db:studio": "npm run db:studio -w backend",
    "ingest": "npm run ingest -w ingestion"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "drizzle-orm": "^0.36.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
